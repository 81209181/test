<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hkt.btu.common.core.dao.mapper.BtuAuditTrailMapper" >

    <insert id="insertAuditTrail" useGeneratedKeys="true" >
        insert into AUDIT_TRAIL (USER_ID, ACTION, DETAIL)
        values (#{userId}, #{action}, #{detail})
    </insert>

</mapper>
