<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Service -->
    <alias name="nocSiteConfigService" alias="siteConfigService"/>
    <alias name="nocSiteConfigService" alias="btuSiteConfigService"/>
    <bean id="nocSiteConfigService" class="com.hkt.btu.noc.core.service.impl.NocSiteConfigServiceImpl"/>

    <alias name="nocUserService" alias="userService"/>
    <alias name="nocUserService" alias="btuUserService"/>
    <bean id="nocUserService" class="com.hkt.btu.noc.core.service.impl.NocUserServiceImpl"/>

    <alias name="nocAuditTrailService" alias="auditTrailService"/>
    <alias name="nocAuditTrailService" alias="btuAuditTrailService"/>
    <bean id="nocAuditTrailService" class="com.hkt.btu.noc.core.service.impl.NocAuditTrailServiceImpl"/>

    <alias name="nocConfigParamService" alias="configParamService"/>
    <bean id="nocConfigParamService" class="com.hkt.btu.noc.core.service.impl.NocConfigParamServiceImpl"/>

    <alias name="nocUserGroupService" alias="userGroupService"/>
    <bean id="nocUserGroupService" class="com.hkt.btu.noc.core.service.impl.NocUserGroupServiceImpl"/>

    <alias name="nocOtpService" alias="otpService"/>
    <bean id="nocOtpService" class="com.hkt.btu.noc.core.service.impl.NocOtpServiceImpl"/>

    <alias name="nocEmailService" alias="emailService"/>
    <bean id="nocEmailService" class="com.hkt.btu.noc.core.service.impl.NocEmailServiceImpl"/>

    <alias name="nocSensitiveDataService" alias="sensitiveDataService"/>
    <bean id="nocSensitiveDataService" class="com.hkt.btu.noc.core.service.impl.NocSensitiveDataServiceImpl"/>

    <alias name="nocSchedulerService" alias="schedulerService"/>
    <bean id="nocSchedulerService" class="com.hkt.btu.noc.core.service.impl.NocSchedulerServiceImpl"/>

    <alias name="nocCronJobProfileService" alias="cronJobProfileService"/>
    <bean id="nocCronJobProfileService" class="com.hkt.btu.noc.core.service.impl.NocCronJobProfileServiceImpl"/>

    <alias name="nocCronJobLogService" alias="cronJobLogService"/>
    <bean id="nocCronJobLogService" class="com.hkt.btu.noc.core.service.impl.NocCronJobLogServiceImpl"/>

    <alias name="nocInputCheckService" alias="inputCheckService"/>
    <bean id="nocInputCheckService" class="com.hkt.btu.noc.core.service.impl.NocInputCheckServiceImpl"/>

    <alias name="nocHealthCheckService" alias="healthCheckService"/>
    <bean id="nocHealthCheckService" class="com.hkt.btu.noc.core.service.impl.NocHealthCheckServiceImpl"/>



    <!-- Populator -->
    <alias name="nocUserBeanPopulator" alias="userBeanPopulator"/>
    <bean id="nocUserBeanPopulator" class="com.hkt.btu.noc.core.service.populator.NocUserBeanPopulator"/>

    <alias name="nocUserGroupPathCtrlBeanPopulator" alias="pathCtrlBeanPopulator"/>
    <bean id="nocUserGroupPathCtrlBeanPopulator" class="com.hkt.btu.noc.core.service.populator.NocUserGroupPathCtrlBeanPopulator"/>

    <alias name="nocUserGroupBeanPopulator" alias="userGroupBeanPopulator"/>
    <bean id="nocUserGroupBeanPopulator" class="com.hkt.btu.noc.core.service.populator.NocUserGroupBeanPopulator"/>

    <alias name="nocOtpBeanPopulator" alias="otpBeanPopulator"/>
    <bean id="nocOtpBeanPopulator" class="com.hkt.btu.noc.core.service.populator.NocOtpBeanPopulator"/>

    <alias name="nocConfigParamBeanPopulator" alias="configParamBeanPopulator"/>
    <bean id="nocConfigParamBeanPopulator" class="com.hkt.btu.noc.core.service.populator.NocConfigParamBeanPopulator"/>

    <alias name="nocCronJobProfileBeanPopulator" alias="cronJobProfileBeanPopulator"/>
    <bean id="nocCronJobProfileBeanPopulator" class="com.hkt.btu.noc.core.service.populator.NocCronJobProfileBeanPopulator"/>

    <alias name="nocCronJobInstBeanPopulator" alias="cronJobInstBeanPopulator"/>
    <bean id="nocCronJobInstBeanPopulator" class="com.hkt.btu.noc.core.service.populator.NocCronJobInstBeanPopulator"/>

    <alias name="nocSiteConfigBeanPopulator" alias="siteConfigBeanPopulator"/>
    <bean id="nocSiteConfigBeanPopulator" class="com.hkt.btu.noc.core.service.populator.NocSiteConfigBeanPopulator"/>


</beans>