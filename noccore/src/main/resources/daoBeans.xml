       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Populator -->
    <alias name="nocAccessRequestEntityPopulator" alias="accessRequestEntityPopulator"/>
    <bean id="nocAccessRequestEntityPopulator" class="com.hkt.btu.noc.core.dao.populator.NocAccessRequestEntityPopulator"/>

    <alias name="nocAccessRequestVisitorEntityPopulator" alias="accessRequestVisitorEntityPopulator"/>
    <bean id="nocAccessRequestVisitorEntityPopulator" class="com.hkt.btu.noc.core.dao.populator.NocAccessRequestVisitorEntityPopulator"/>

    <alias name="nocAccessRequestEquipEntityPopulator" alias="accessRequestEquipEntityPopulator"/>
    <bean id="nocAccessRequestEquipEntityPopulator" class="com.hkt.btu.noc.core.dao.populator.NocAccessRequestEquipEntityPopulator"/>

    <alias name="nocCronJobLogEntityPopulator" alias="cronJobLogEntityPopulator"/>
    <bean id="nocCronJobLogEntityPopulator" class="com.hkt.btu.noc.core.dao.populator.NocCronJobLogEntityPopulator"/>


</beans>